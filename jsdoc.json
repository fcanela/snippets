{
  "comment": {
    "prefix": "/**",
    "body": [
      "/**",
      " * ${1:What it does}.",
      " *",
      " * @param ${3:name} - ${4:Parameter description.}",
      " * @returns ${2:Type and description of the returned object.}",
      " *",
      " * @example",
      " * ```",
      " * ${5:Write me later.}$0",
      " * ```",
      " */"
    ],
    "description": "A full JSDoc comment with description, parameters, return, and example."
  },
  "comment simple": {
    "prefix": "/*",
    "body": [
      "/**",
      " * ${1:Comment.}$0",
      " */"
    ],
    "description": "A simpple JSDoc comment."
  },
  "dosctring parameter": {
    "prefix": "@par",
    "description": "Parameter DocString",
    "body": "@param (${1:type}) ${2:name} ${3:description};"
  },
  "dosctring return": {
    "prefix": "@ret",
    "description": "Return DocString",
    "body": "@return (${1:type}) ${2:description};"
  }
}
