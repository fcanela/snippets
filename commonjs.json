{
  "default export": {
    "prefix": "me",
    "description": "CommonJS default export",
    "body": "module.exports = ${1};"
  },
  "require": {
    "prefix": "cr",
    "description": "Constant Assigment to Require",
    "body": "const ${1:name} = require('${2:$1}');"
  },
  "require member": {
    "prefix": "crd",
    "description": "Constant Assigment to Destructured Require",
    "body": "const { ${1:name} } = require('${2}');"
  }
}
