{
  "default export": {
    "prefix": "mo",
    "description": "CJS default export",
    "body": "module.exports = ${1};"
  },
  "require": {
    "prefix": "cr",
    "description": "CJS constant from require",
    "body": "const ${1:name} = require('${2:$1}');"
  },
  "require member": {
    "prefix": "crd",
    "description": "CJS constant from require (destructured)",
    "body": "const { ${1:name} } = require('${2}');"
  }
}
