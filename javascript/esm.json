{
  "import default": {
    "prefix": "im",
    "description": "ESM default import",
    "body": "import ${1} from '${1}'"
  },
  "import all as": {
    "prefix": "imas",
    "description": "ESM import all as",
    "body": "import * as ${1} from '${2:$1}'"
  },
  "import member": {
    "prefix": "imd",
    "description": "ESM import (destructured)",
    "body": "import { ${1: member} } from '${2:package}'"
  }
}
