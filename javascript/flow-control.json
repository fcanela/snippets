{
  "if block": {
    "prefix": "if",
    "description": "If Clause",
    "body": [
      "if (${1}) {",
      "\t${2}",
      "}"
    ]
  },
  "if one line": {
    "prefix": "ifl",
    "description": "If Clause (one line)",
    "body": "if (${1}) "
  },
  "early return if": {
    "prefix": "ifr",
    "description": "Early return",
    "body": "if (${1}) return${2};"
  },
  "ternary": {
    "prefix": "ter",
    "description": "Ternary",
    "body": "${1} ? ${2} : ${3};"
  }
}
